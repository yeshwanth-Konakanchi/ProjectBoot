package com.nivtek.training.wrapper;

public class DemoWrapperTypes {

  /**
   * @param args
   */
  public static void main(String[] args) {
    
    int intNumber = Integer.parseInt(args[0]);
    System.out.println("int number: " + intNumber);
    
    float floatNumber = Float.parseFloat(args[1]);
    System.out.println("float number: " + floatNumber);
    
  }


}
