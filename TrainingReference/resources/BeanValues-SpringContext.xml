<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
 
    <bean id="firstFileGen" class="com.zenolocity.training.spring.beanvalues.FileNameGenerator">
        <property name="name" value="Ravi" />
        <property name="type" value="Person" />
    </bean>
    
    <bean id="secondFileGen" class="com.zenolocity.training.spring.beanvalues.FileNameGenerator" scope="prototype">
        <property name="name" value="Ravi" />
        <property name="type" value="Animal" />
    </bean>
    
    <bean id="thirdFileGen" class="com.zenolocity.training.spring.beanvalues.FileNameGenerator">
        <property name="name" value="Sola" />
        <property name="type" value="Student" />
    </bean>
 
</beans>