package com.crmindz.bcjmay2017.flowcontrol.lab9;

import java.util.Scanner;

/**
 * @author Yeshwanth Konakanchi
 *This class will check whether a right angled triangle can be formed or not 
 * with given sides as input.
 */
public class RightAngledTriangle {
	
	/**
	 * @param args
	 * This method will take three sides as input and 
	 * checks whether a right angled triangle can be formed or not.
	 */
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int c = scan.nextInt();
		int b = scan.nextInt();
		int a = scan.nextInt();
		
		c *= c;
		b *= b;
		a *= a;

		if (c == a + b) {
			System.out.println("they could represent the sides of a right triangle");
		} else
			System.out.println("they could not represent as sides of a right triangle");
		
		scan.close();
	}

}
